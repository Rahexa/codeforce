import java.util.Scanner;
public class test {
	public static void main(String[] args) {
int a;
int b;
int c;
int n;
	Scanner sc = new Scanner(System.in)	;
	System.out.println();
n = sc.nextInt();
		
		
		for(int i = 0;i <n;i++) {
			
			
			a = sc.nextInt();
			b = sc.nextInt();
			c = sc.nextInt();
			
			
			
			if(a+b == c) {
				System.out.println("+");
			}
			
			else {System.out.println("-");}
		}
		
		
		
		
		
		
		
		
		
		
		
		
		
		
	}
}
