import java.util.Scanner;
 
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.println();
String  input = sc.nextLine();
 
 
String result = input.replaceAll("WUB" , " ").trim();
 
 
System.out.println(result);
 
        sc.close();
    }
}
